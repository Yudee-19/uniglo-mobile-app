{
    "cli": {
        "version": ">= 18.0.4",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "EXPO_PUBLIC_API_BASE_URL": "https://uniglow-inventory-service-dev.caratlogic.com/api"
            }
        },
        "preview": {
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EXPO_PUBLIC_API_BASE_URL": "https://uniglow-inventory-service-dev.caratlogic.com/api"
            }
        },
        "production": {
            "autoIncrement": true,
            "env": {
                "EXPO_PUBLIC_API_BASE_URL": "https://uniglow-inventory-service-dev.caratlogic.com/api"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
